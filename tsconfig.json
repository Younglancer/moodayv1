{
    "extends": "@expo/cli/tsconfig.json",
    "compilerOptions": {
      "strict": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "paths": {
        "@/*": [
          "./*"
        ]
      }
    },
    "include": [
      "**/*.ts",
      "**/*.tsx",
      ".expo/types/**/*.ts",
      "expo-env.d.ts"
    ],
    "exclude": ["node_modules"]
  }
  